package cli

import (
	"{{ cookiecutter.project_name|lower }}/server"

	"github.com/spf13/cobra"
)

var runserverCmd = &cobra.Command{
	Use:   "runserver",
	Short: "Run the user email service REST API",
	Run: func(cmd *cobra.Command, args []string) {
		server.Configure()
		s := server.New()
		server.RunServer(s)
	},
}
